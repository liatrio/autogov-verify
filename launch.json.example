{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {
        "GITHUB_TOKEN": "your-token-here"
      },
      "args": [
        "-o",
        "organization-name",
        "-w",
        "workflow-repo-name",
        "-d",
        "sha256:digest",
        "-i",
        "https://github.com/org/repo/.github/workflows/workflow.yaml@ref",
        "-q"
      ]
    }
  ]
}
